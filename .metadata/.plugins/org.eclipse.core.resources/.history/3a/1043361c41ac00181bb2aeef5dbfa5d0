package com.dev.ad3i;

public class FindOutputDemo3 {

	public static void main(String[] args)
    {
        DemoClass3 obj1 = new DemoClass3();
        DemoClass3 obj2 = obj1;
 
        obj1.a += 1;
        obj1.b += 1;
 
        System.out.println ("values of obj1 : ");
        obj1.print();
        System.out.println ("values of obj2 : ");
        obj2.print();
 
    }
}

class DemoClass3
{
    int a, b;
     
    DemoClass3()
    {
        a = 10;
        b = 20;
    }
     
    public void print()
    {
        System.out.println ("a = " + a + " b = " + b + "n");
    }
}
