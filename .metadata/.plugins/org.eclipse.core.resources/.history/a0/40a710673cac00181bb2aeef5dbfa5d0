package com.dev.ad3i;

public class InheritanceDemo2 {

	public static void main(String[] args) 
    {
        new SubSubAlpha();
        System.out.println(s);
    }
}

class Alpha 
{
    static String s = " ";
    protected Alpha() 
    {
        s += "alpha ";
    }
}
class SubAlpha extends Alpha 
{
    private SubAlpha() 
    {
        s += "sub ";
    }
}
 
class SubSubAlpha extends Alpha 
{
    SubSubAlpha() 
    {
        s += "subsub ";
    }
    
}
