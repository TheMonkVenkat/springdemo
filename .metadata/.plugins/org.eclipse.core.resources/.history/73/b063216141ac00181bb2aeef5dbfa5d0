package com.dev.ad3i;

public class FindOutputDemo4 {

	public static void main(String[] args)
    {
        OuterClass obj = new OuterClass();
        obj.func();
        obj.get();
 
    }
}

class OuterClass
{
    int a = 1;
 
    void func()
    {
        demo obj = new demo();
        obj.display();
    }
 
 
    class demo
    {
        int b = 2;
 
        void display()
        {
            System.out.println("na = " + a);
        }
    }
 
    void get()
    {
        System.out.println("nb = " + b);
    }
}